        </examples>
        <mappings>
            <wn version="3.0">7</wn>
            <omega/><pb>NM</pb>
        </mappings>
        <SENSE_META clarity=""/>
    </sense>
     
    
    <sense group="1" n="7" name="oarsman in a rowing shell who sets the pace" type="">
