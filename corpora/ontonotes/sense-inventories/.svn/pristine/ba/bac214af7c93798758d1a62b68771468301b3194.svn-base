    <sense group="1" n="2" name="verb tense" type="Linguistic">
        <commentary>
            a verb tense that expresses actions or states in the future
        </commentary>
        <examples>
            The paper was written in future tense.
        </examples>
        <mappings>
            <wn version="2.0">2</wn>
            <omega/>
            <pb/>
        </mappings>
        <SENSE_META clarity=""/>
    </sense>
    
    <sense group="1" n="3" name="commodities" type="">
