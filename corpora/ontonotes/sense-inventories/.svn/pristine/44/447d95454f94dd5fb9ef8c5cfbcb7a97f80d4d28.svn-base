         <wn lemma="walk_around" version="3.0">1,2,4</wn>
         <wn lemma="walk_away" version="3.0">1</wn>
         <wn lemma="walk_in" version="3.0">1</wn>
         <wn lemma="walk_off" version="3.0">2</wn>
         <wn lemma="walk_on_air" version="3.0">1</wn>
