        </mappings>
        <SENSE_META clarity=""/>
    </sense>
     
    
    <sense group="1" n="3" name="the trajectory along which something is moving" type="">
        <commentary>
            PATH[+location][+spatial][+trajectory][+moving]
        </commentary>
