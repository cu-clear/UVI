            <wn version="3.0">4,7</wn>
            <omega/><pb/>
        </mappings>
        <SENSE_META clarity=""/>
    </sense>
     
    
    <sense group="1" n="4" name="a cylindrically-shaped specimen or sample, usually geological" type="">
